									<label class="sr-only" for="form-username">UserName</label> <input
										type="text" name="uname" placeholder="Username"
										class="form-username form-control" id="form-username">
								</div>
								<div class="form-group">
									<label class="sr-only" for="form-password">Password</label> <input
										type="password" name="pwd" placeholder="Password"
										class="form-password form-control" id="form-password">
								</div>
								<button type="submit" class="btn" value="submit">Login</button>
							</form>
						</div>
					</div>
				</div>
				<div class="form-elements-divider"></div>
				<div class="l-form-10-questions">
